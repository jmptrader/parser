/[ \t\n]/   { /* Skip white space */ }
/[0-9]+/    { lval.int_t, _ = strconv.Atoi(yylex.Text()); return NUM }
/\+/        { return '+' }
/\-/        { return '-' }
/\*/        { return '*' }
/\//        { return '/' }
/\(/        { return '(' }
/\)/        { return ')' }
/SELECT|select/    { return KW_SELECT }
/./         {
                fmt.Printf(yylex.Text())
                //panic(fmt.Sprintf("There's an unexpected token '%s' here", yylex.Text()))
            }
//

package main

import (
    "os"
    "strconv"
    "fmt"
)

func main() {
    yyParse(NewLexer(os.Stdin))
}